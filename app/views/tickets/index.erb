<table style="width: 75%">
<tr>
    <th>Title</th>
    <th>Submitter</th>
    <th>Owner</th>
    <th></th>
</tr>
<% @tickets.each do |ticket| %>
<tr>
    <td><%= link_to ticket.title, ticket_path(ticket) %></td>
    <td><%= ticket.user.full_name %></td>
    <td><%= ticket.admin.full_name %></td>
    <td>Claim or Assign</td>
</tr>
<% end %>